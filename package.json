    "redux-logger": "^2.6.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
